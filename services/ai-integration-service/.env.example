APP_NAME="AidlY AI Integration Service"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8006
APP_TIMEZONE=UTC

LOG_CHANNEL=stack
LOG_SLACK_WEBHOOK_URL=

# Database Configuration
DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=aidly
DB_USERNAME=aidly_user
DB_PASSWORD=aidly_secret_2024

# Cache and Session
CACHE_DRIVER=redis
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=redis_secret_2024
REDIS_PORT=6379

# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=aidly_admin
RABBITMQ_PASSWORD=rabbitmq_secret_2024
RABBITMQ_VHOST=/

# AI Provider Configuration
AI_DEFAULT_PROVIDER=openai

# OpenAI Configuration
OPENAI_ENABLED=false
OPENAI_API_KEY=your-openai-api-key
OPENAI_ORGANIZATION=
OPENAI_MODEL_CATEGORIZATION=gpt-3.5-turbo
OPENAI_MODEL_PRIORITIZATION=gpt-3.5-turbo
OPENAI_MODEL_RESPONSE=gpt-4
OPENAI_MODEL_SENTIMENT=gpt-3.5-turbo
OPENAI_MODEL_SUMMARIZATION=gpt-3.5-turbo

# Anthropic Configuration
ANTHROPIC_ENABLED=false
ANTHROPIC_API_KEY=your-anthropic-api-key
ANTHROPIC_MODEL=claude-2

# Google Gemini Configuration
GEMINI_ENABLED=false
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-pro

# Custom AI Provider
CUSTOM_AI_ENABLED=false
CUSTOM_AI_ENDPOINT=
CUSTOM_AI_API_KEY=
CUSTOM_AI_HEADERS=

# Processing Configuration
AI_MAX_RETRIES=3
AI_RETRY_DELAY=60
AI_BATCH_SIZE=10
AI_ASYNC_ENABLED=true
AI_CACHE_TTL=3600
AI_RATE_LIMIT=100

# Feature Flags
AI_FEATURE_AUTO_CATEGORIZATION=false
AI_FEATURE_AUTO_PRIORITIZATION=false
AI_FEATURE_RESPONSE_SUGGESTIONS=false
AI_FEATURE_SENTIMENT_ANALYSIS=false
AI_FEATURE_ENTITY_EXTRACTION=false
AI_FEATURE_SUMMARIZATION=false
AI_FEATURE_KB_GENERATION=false
AI_FEATURE_SMART_ROUTING=false
AI_FEATURE_PREDICTIVE_ANALYTICS=false

# Webhook Configuration
WEBHOOK_SIGNING_SECRET=your-webhook-secret
WEBHOOK_TIMEOUT=30
WEBHOOK_RETRY_ENABLED=true
WEBHOOK_RETRY_MAX_ATTEMPTS=3
WEBHOOK_RETRY_DELAY=60

# Webhook Endpoints
WEBHOOK_OPENAI_ENABLED=true
WEBHOOK_OPENAI_SECRET=
WEBHOOK_OPENAI_VERIFY=true

WEBHOOK_ANTHROPIC_ENABLED=true
WEBHOOK_ANTHROPIC_SECRET=
WEBHOOK_ANTHROPIC_VERIFY=true

WEBHOOK_GEMINI_ENABLED=true
WEBHOOK_GEMINI_SECRET=
WEBHOOK_GEMINI_VERIFY=true

WEBHOOK_CUSTOM_ENABLED=true
WEBHOOK_CUSTOM_SECRET=
WEBHOOK_CUSTOM_VERIFY=false

# Webhook Security
WEBHOOK_ALLOWED_IPS=
WEBHOOK_RATE_LIMIT_ENABLED=true
WEBHOOK_RATE_LIMIT_MAX=100
WEBHOOK_RATE_LIMIT_WINDOW=60

# Monitoring
AI_MONITORING_ENABLED=true
AI_LOG_REQUESTS=true
AI_LOG_RESPONSES=false
AI_METRICS_ENABLED=true
AI_ALERT_ON_FAILURE=true

# Security
AI_ENCRYPT_API_KEYS=true
AI_MASK_SENSITIVE_DATA=true
AI_ALLOWED_IPS=
AI_WEBHOOK_SECRET=